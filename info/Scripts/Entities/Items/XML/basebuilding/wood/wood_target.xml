<item name="wood_target" class="Placeable" category="any">

	<params>
		<param name="selectable" value="0"/>
		<param name="mass" value="2"/>
		<param name="stack_size" value="5"/>
		<param name="display_name" value="@wood_target"/>
		<param name="description" value="@wood_targetDescription"/>
	</params>

	<base_part class="static" type="12" name="wood_target" max_health="5000">
		<static filename="Objects/basebuilding/other/wood_target/wood_target.cgf"/>

		<!-- Modified for lower bullet damage (default = 0.1) and higher melee damage due to nature of use -->
		<damage_multipliers>
			<!-- standard hit types -->
			<damage_multiplier damage_type="default" multiplier="0.1"/>
			<damage_multiplier damage_type="collision" multiplier="0.0"/>
			<damage_multiplier damage_type="explosion" multiplier="1.0"/>
			<damage_multiplier damage_type="frag" multiplier="1.0"/>
			<damage_multiplier damage_type="fire" multiplier="1.0"/>
			<damage_multiplier damage_type="melee" multiplier="5.0"/>
			<damage_multiplier damage_type="repair" multiplier="0.0"/> <!-- repair weapons do nothing -->
			<!-- miscreated hit types -->
			<damage_multiplier damage_type="blunt" multiplier="5.0"/>
			<damage_multiplier damage_type="flare" multiplier="0.0"/>
			<damage_multiplier damage_type="hammer" multiplier="5.0"/>
			<damage_multiplier damage_type="hatchet" multiplier="5.0"/>
			<damage_multiplier damage_type="knife" multiplier="5.0"/>
			<damage_multiplier damage_type="pickaxe" multiplier="5.0"/>
			<damage_multiplier damage_type="torch" multiplier="1.0"/>
		</damage_multipliers>
	</base_part>

	<geometry>
		<thirdperson name="Objects/basebuilding/wood/wood_part.cgf"/>
	</geometry>

	<storage>
		<locations>
			<location category="any"/>
		</locations>
	</storage>

	<steam>
		<param name="tags" value="Category:BaseBuilding"/>
		<param name="icon_url" value="http://miscreatedgame.com/shots/item_images/wood_target_200.png?crc=A30BF486"/>
		<param name="store_hidden" value="0"/>
		<param name="amcoins" value="1"/>
		<param name="tradable" value="0"/>
		<param name="name_color" value="FF001E"/>
		<param name="itemdefid" value="30197"/>
		<param name="marketable" value="0"/>
		<param name="store_tags" value="Category:BaseBuilding"/>
		<param name="background_color" value="2A2A2A"/>
		<param name="hidden" value="0"/>
		<param name="icon_url_large" value="http://miscreatedgame.com/shots/item_images/wood_target_2048.png?crc=A30BF486"/>
	</steam>
</item>