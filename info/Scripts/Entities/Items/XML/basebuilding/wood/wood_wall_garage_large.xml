<item name="wood_wall_garage_large" class="Placeable" category="any">

	<params>
		<param name="selectable" value="0"/>
		<param name="mass" value="5"/>
		<param name="stack_size" value="5"/>
		<param name="display_name" value="Wood Wall Garage Large"/>
		<param name="description" value="A large wood wall with an railing system for a garage door."/>
	</params>

	<base_part class="static" type="298" name="wood_wall_garage_large" repair_sound="Play_wall_repair">
		<static filename="objects/basebuilding/wood/wood_wall_garage_large.cgf"/>
	</base_part>
	
	<explosion>
		<param name="sound_radius" value="50"/> <!-- max radius the AI will react to the explosion sound -->
		<param name="hole_size" value="3"/> <!-- radius to remove entities within -->
		<param name="effect" value="misc.basebuilding.wall_destroyed"/>
		<param name="effect_scale" value="1"/>
	</explosion>

	<geometry>
		<thirdperson name="objects/basebuilding/wood/wood_part.cgf"/>
	</geometry>

	<storage>
		<locations>
			<location category="any"/>
		</locations>
	</storage>

</item>