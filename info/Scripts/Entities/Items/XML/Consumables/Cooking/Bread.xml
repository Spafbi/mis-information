<item name="Bread" class="Consumable" category="any">

	<params>
		<param name="selectable" value="0"/>
		<param name="mass" value="0.3"/>
		<param name="display_name" value="@Bread"/>
		<param name="description" value="@BreadDescription"/>
	</params>

	<geometry>
		<thirdperson name="Objects/consumables/bread/bread.cgf"/>
	</geometry>

	<consumable>
	    <param name="consumePartial" value="1"/>
