			<Views>
				<View class="FirstPerson" canRotate="1" relaxTimeMax="0.1" rotationMin="-40,0,-60" hidePlayer="0" rotationMax="20,0,70" followSpeed="16">
					<FirstPerson offset="0,0,0" characterBone="Bip01 Camera" relativeToHorizon="0.0"/>
				</View>
