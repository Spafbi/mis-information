   </ATLSwitchState>
   <ATLSwitchState atl_name="grass">
    <WwiseSwitch wwise_name="surface">
     <WwiseValue wwise_name="grass"/>
    </WwiseSwitch>
   </ATLSwitchState>
   <ATLSwitchState atl_name="gravel">
    <WwiseSwitch wwise_name="surface">
     <WwiseValue wwise_name="gravel"/>
