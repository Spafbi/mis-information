   <ATLSwitchState atl_name="wood_solid">
    <WwiseSwitch wwise_name="surface">
     <WwiseValue wwise_name="wood_solid"/>
    </WwiseSwitch>
   </ATLSwitchState>
   <ATLSwitchState atl_name="metal">
    <WwiseSwitch wwise_name="surface">
     <WwiseValue wwise_name="metal"/>
    </WwiseSwitch>
   </ATLSwitchState>
   <ATLSwitchState atl_name="mud">
    <WwiseSwitch wwise_name="surface">
