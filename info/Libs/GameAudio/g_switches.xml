   </ATLSwitchState>
   <ATLSwitchState atl_name="water_deep">
    <WwiseSwitch wwise_name="surface">
     <WwiseValue wwise_name="water_deep"/>
    </WwiseSwitch>
   </ATLSwitchState>
   <ATLSwitchState atl_name="water_shallow">
    <WwiseSwitch wwise_name="surface">
     <WwiseValue wwise_name="water_shallow"/>
