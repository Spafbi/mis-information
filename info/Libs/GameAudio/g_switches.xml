   </ATLSwitchState>
  </ATLSwitch>
  <ATLSwitch atl_name="explosion_size">
   <ATLSwitchState atl_name="large">
    <WwiseSwitch wwise_name="explosion_size">
     <WwiseValue wwise_name="large"/>
    </WwiseSwitch>
   </ATLSwitchState>
   <ATLSwitchState atl_name="medium">
    <WwiseSwitch wwise_name="explosion_size">
     <WwiseValue wwise_name="medium"/>
    </WwiseSwitch>
   </ATLSwitchState>
