   </ATLSwitchState>
  </ATLSwitch>
  <ATLSwitch atl_name="underwater">
   <ATLSwitchState atl_name="above_water">
    <WwiseState wwise_name="underwater">
     <WwiseValue wwise_name="above_water"/>
    </WwiseState>
   </ATLSwitchState>
   <ATLSwitchState atl_name="None">
    <WwiseState wwise_name="underwater">
     <WwiseValue wwise_name="None"/>
    </WwiseState>
   </ATLSwitchState>
   <ATLSwitchState atl_name="underwater">
    <WwiseState wwise_name="underwater">
     <WwiseValue wwise_name="underwater"/>
    </WwiseState>
   </ATLSwitchState>
  </ATLSwitch>
 </AudioSwitches>
</ATLConfig>
