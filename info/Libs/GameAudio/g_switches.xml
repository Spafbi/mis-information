    </WwiseSwitch>
   </ATLSwitchState>
  </ATLSwitch>
  <ATLSwitch atl_name="house_in_out">
   <ATLSwitchState atl_name="house_inside">
    <WwiseSwitch wwise_name="v_listener">
     <WwiseValue wwise_name="inside"/>
    </WwiseSwitch>
   </ATLSwitchState>
   <ATLSwitchState atl_name="house_outside">
    <WwiseSwitch wwise_name="v_listener">
