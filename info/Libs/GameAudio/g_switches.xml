<ATLConfig atl_name="g_switches">
 <AudioSwitches>
  <ATLSwitch atl_name="1stOr3rdP">
   <ATLSwitchState atl_name="1stP">
    <WwiseSwitch wwise_name="FirstPerson_ThirdPerson">
     <WwiseValue wwise_name="first_person"/>
    </WwiseSwitch>
   </ATLSwitchState>
   <ATLSwitchState atl_name="3rdP">
    <WwiseSwitch wwise_name="FirstPerson_ThirdPerson">
     <WwiseValue wwise_name="third_person"/>
    </WwiseSwitch>
   </ATLSwitchState>
  </ATLSwitch>
  <ATLSwitch atl_name="ObstrOcclCalcType">
   <ATLSwitchState atl_name="Ignore">
    <ATLSwitchRequest atl_name="ObstructionOcclusionCalculationType">
     <ATLValue atl_name="ignore"/>
    </ATLSwitchRequest>
   </ATLSwitchState>
   <ATLSwitchState atl_name="SingleRay">
    <ATLSwitchRequest atl_name="ObstructionOcclusionCalculationType">
     <ATLValue atl_name="single_ray"/>
    </ATLSwitchRequest>
   </ATLSwitchState>
   <ATLSwitchState atl_name="MultiRay">
    <ATLSwitchRequest atl_name="ObstructionOcclusionCalculationType">
     <ATLValue atl_name="multi_ray"/>
    </ATLSwitchRequest>
   </ATLSwitchState>
  </ATLSwitch>
  <ATLSwitch atl_name="SurfaceType">
   <ATLSwitchState atl_name="carpet">
    <WwiseSwitch wwise_name="surface">
     <WwiseValue wwise_name="carpet"/>
    </WwiseSwitch>
   </ATLSwitchState>
   <ATLSwitchState atl_name="concrete">
    <WwiseSwitch wwise_name="surface">
     <WwiseValue wwise_name="concrete"/>
