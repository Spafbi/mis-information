	<Effect name="bullet_impact_default" delay="0.05">
		<Audio trigger="Play_bullet_impact_default" />
		<Particle>
			<Name>bullet.hit_default.a</Name>
		</Particle>
		<RandEffect>
			<Decal minscale="0.04" maxscale="0.06">
				<Material>materials/decals/dirt/ground</Material>
