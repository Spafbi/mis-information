<!-- See http://freesdk.crydev.net/display/SDKDOC4/Flash+UI+System for a definition of this file -->
<UIElements name="VOIP">

	<UIElement name="VOIP"  mouseevents="0" keyevents="0" cursor="0" controller_input="0" render_lockless="1">

		<GFx file="VOIP/VOIP.gfx" layer="1">
			<Constraints>
				<Align mode="dynamic" valign="center" halign="left" scale="1" max="0" />
			</Constraints>
		</GFx>

		<!-- Can get or set a variable -->
		<variables>
		</variables>

		<!-- Can get or set an array -->
		<arrays>
		</arrays>

		<!-- Functions that can be invoked into actionscript from lua -->
		<functions>
			<function name="SetVOIPState" funcname="_root.setVOIPState">
				<param name="state" desc="The current state of VOIP"/>
			</function>
			<function name="SetVOIPLoudness" funcname="_root.setVOIPLoudness">
				<param name="loudness" desc="The current loudness of VOIP 0-1"/>
			</function>
		</functions>

		<!-- Events fired from actionsript into lua -->
		<events>
		</events>

		<movieclips>
		</movieclips>

	</UIElement>
	
</UIElements>